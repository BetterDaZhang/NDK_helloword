<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="example.zhangjunling.com.ndk_hellowork">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HelloWordJniActivity" />
        <activity android:name=".ExceptionJniActivity" />
        <activity android:name=".ReferenceJniActivity" />
        <activity android:name=".DebugActivity" />
        <activity android:name=".BionicActivity" />
        <activity android:name=".ThreadActivity"></activity>
    </application>

</manifest>