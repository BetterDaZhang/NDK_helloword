<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="example.zhangjunling.com.ndk_hellowork.MainActivity"
    android:orientation="vertical">
    <Button
        android:id="@+id/bt_view_hellojni"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="View Hello Jni"/>
    <Button
        android:id="@+id/bt_view_exception"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="View Excepton"/>
    <Button
        android:id="@+id/bt_view_reference"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="View Reference"/>
    <Button
        android:id="@+id/bt_view_debug"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="View Debug"/>
    <Button
        android:id="@+id/bt_view_bionic"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="View Bionic"/>
    <Button
        android:id="@+id/bt_view_thread"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="View Thread"/>
    <Button
        android:id="@+id/bt_view_echo_client"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="View Echo Client"/>
    <Button
        android:id="@+id/bt_view_echo_server"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="View Echo Server"/>
    <Button
        android:id="@+id/bt_view_echo_local"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="View Echo Local"/>

</LinearLayout>
